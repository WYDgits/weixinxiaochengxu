<!--pages/person/person.wxml-->

<view>
  <view class='header'>
    欢迎使用湖南科技大学网络信息服务
  </view>
  <view class="weui-cells weui-cells_after-title">
    <navigator url="../modify_psw/modify_psw"  class="weui-cell weui-cell_access" hover-class="weui-cell_active">
      <view class="weui-cell__bd">{{modify_psw}}</view>
      <view class="weui-cell__ft weui-cell__ft_in-access"></view>
    </navigator>
    <view   bindtap='logout' class="weui-cell weui-cell_access" hover-class="weui-cell_active">
      <view class="weui-cell__bd">{{logout}}</view>
      <view class="weui-cell__ft weui-cell__ft_in-access"></view>
    </view>
  </view>
